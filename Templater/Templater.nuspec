<?xml version="1.0"?>
<package >
  <metadata>
    <id>nTemplater</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>Nicolas Penin</authors>
    <owners>Nicolas Penin</owners>
    <projectUrl>https://github.com/npenin/templater/tree/master</projectUrl>
