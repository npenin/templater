    <copyright>Copyright 2020</copyright>
    <dependencies>
        <dependency id="Jint" version="2.11.58"/>
        <dependency id="Jurassic" version="3.1.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\netstandard2.0\Templater.dll" target="lib\net4\Templater.dll" />
  </files>
</package>